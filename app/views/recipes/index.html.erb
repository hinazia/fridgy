<div class="suggested-recipes">
  <h1 style="margin-top: 80px;">Choose one of these recipes:</h1>
  <% @recipes.each do |recipe| %>
    <div class=''>
      <%= link_to recipe_path(recipe) do  %>
        <div class="recipe-body">
          <%= cl_image_tag recipe.photo.key, class: 'recipe-wrapper' %>

          <h2><%= recipe.title %></h2>
        </div>
      <% end %>
    </div>
    <%end%>
</div>
